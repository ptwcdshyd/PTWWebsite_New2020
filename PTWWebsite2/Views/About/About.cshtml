<script src="../js/jquery-3.3.1.min.js"></script>
<script>
            $(document).ready(function () {
                LoadProfile();


            });
        function LoadProfile() {
            debugger;
                $.ajax({
                    url: "../"+"@Model.Culture"+"/AboutProfile",
                    type: "GET",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (result) {
                        var htmlContent = "";
                        var count = 1;
                        $.each(result.Profiles, function (result, item) {
                            if (count == 1 || (count-1) % 3 == 0) {
                                htmlContent = htmlContent + '<div class="grid">'
                            }
                            htmlContent = htmlContent +
                                '<figure class="effect-sadie">' +
                                '<img src="' + item.ImgPath + '" alt="img02" />' +
                                '<figcaption>' +
                                '<p>' + item.Title + '<br/>' + item.Description + '</p>' +
                                '<a href="#">View more</a>' +
                                '</figcaption>' +
                                '</figure>';

                            if (count % 3 == 0) {
                                htmlContent = htmlContent + '</div>'
                            }

                            count++;
                        });
                        if (result.Profiles.length % 3 != 0) {
                            htmlContent = htmlContent + '</div>'
                        }
                        $('#Profiles').append(htmlContent);

                    },
                    error: function (err) {

                    }
                });

            }


</script>


<script src="../JS/count-up.min.js"></script>

<script>
    var cu = new counterUp({
        start: 0,
        duration: 6000,
        intvalues: true,
        interval: 100
    });
    cu.start();
</script>

<script>
    $(document).ready(function () {
        RemoveLastSlashInUrl();
    });

    function RemoveLastSlashInUrl() {
        var str = window.location.href.replace(window.location.origin, "");
        var lastchar = str.charAt(str.length - 1);
        if (lastchar == '/') {
            str = str.slice(0, -1);
            window.location = window.location.origin + str;
        }
    }
</script>




