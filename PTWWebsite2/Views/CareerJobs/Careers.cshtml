                        $(this).parent().parent().show();
                        $("#search").val("");
                    }
                    else {
                        $(this).parent().parent().hide();
                    }


                }
            }
            else {
                $(this).parent().parent().hide();
            }
        });

        Binddepts();
        if ($("#ddldepts").val() != "")
            $(".whr-item ul.whr-info:not(:has(li.whr-dept))").parent().hide();

    });



