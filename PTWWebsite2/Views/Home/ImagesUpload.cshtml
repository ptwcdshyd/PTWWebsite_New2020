@model PTW.DataAccess.Models.Services

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ImagesUpload</title>
</head>
<body>

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <script src="~/js/jquery-3.3.1.min.js"></script>
    <div class="row">
        <div class="col-md-4">
            <form asp-controller="Image" class="mt-3" asp-action="ImagesUpload" enctype="multipart/form-data" method="post">

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                 @*<label asp-for="Customerexperience" cl*@
                    <input asp-for="Customerexperience" class="form-control" />
                </div>
                <div class="form-group">
                    <input asp-for="Audioproduction" class="form-control" />
                </div>
                <div class="form-group">
                    <input asp-for="Engineering" class="form-control" />
                </div>
                <div class="form-group">
                    <input asp-for="Qualityassurance" class="form-control" />
                </div>
                <div class="form-group">
                    <input asp-for="Localization" class="form-control" />
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Index">Back to List</a>
    </div>

</body>
</html>
