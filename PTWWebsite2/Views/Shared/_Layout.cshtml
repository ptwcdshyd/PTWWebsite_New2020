        jQuery(document).ready(function (e) {
            var lan='@(ViewContext.RouteData.Values["culture"]!=null?ViewContext.RouteData.Values["culture"].ToString():"en-US")';
            //console.log(lan);
            $("#Languages").val(lan);

            "use strict";
            if (e("form").on("focus", "input,textarea", function () {
                e(this).prev("i.fa").addClass("focus")
            }).on("blur", "input,textarea", function () {
                e(this).prev("i.fa").removeClass("focus")
            }), e("#rex-sticke-nav").sticky({
                topSpacing: 0
            }), e("body").hasClass("single")) {
                var n = e("#rex-navigation li a");
                n.each(function () {
                    var a = e(this).attr("href");
                    e(this).attr("href", "./" + a)
                })
            }
            e(".backtop").hide(), e(window).on("scroll", function () {
                e(this).scrollTop() > 400 ? e(".backtop").fadeIn() : e(".backtop").fadeOut()
            }), e(".backtop").on("click", function () {
                return e("html, body").animate({
                    scrollTop: 0
                }, 800), !1
            }), (new WOW).init();

        });

        function hover(element, src) {
            var source = '/' + src.trim().replace('../', '');
            $(element).find("img").attr('src', source);
            $(element).css('cssText', 'cursor: pointer !important');
        }

        function colorhover(element, src) {
            $(element).find("img").attr('src', src);
        }

        function unhover(element, src) {
            var source = '/' + src.trim().replace('../', '');
            $(element).find("img").attr('src', source);
        }



        $('[data-toggle="slide-collapse"]').on('click', function () {
            $navMenuCont = $($(this).data('target'));
            $navMenuCont.animate({ right: 'toggle' }, 350);
        });

        function myFunction(x) {
            x.classList.toggle("change");
        }

        function scroller(detail) {
            $('html,body').animate({
                scrollTop: $(detail).offset().top
            },
                'slow');

        }

        function languageChange(language) {
            debugger;
            $('.ddlLanguage').val(language).change();
            var selectedText = $('.ddlLanguage')[0].options[$('.ddlLanguage')[0].selectedIndex].innerHTML.split("\n")[0];
            $('#hLanguage').text(selectedText);
            $('#fLanguage').text(selectedText);
        }



        function serviceChange(service) {
            $('.ddlServices').val(service).change();
            var selectedText = $('.ddlServices')[0].options[$('.ddlServices')[0].selectedIndex].innerHTML;
            $('#hService').text(selectedText);
        }

        function showHide(showID, hideID) {

            $('#' + showID).css('display', 'block');
            $('#' + hideID).css('display', 'none');
        }
    </script>
